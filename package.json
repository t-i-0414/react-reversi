{
  "name": "react-reversi",
  "version": "1.8.0",
  "homepage": "https://github.com/t-i-0414/react-reversi#readme",
  "bugs": {
    "url": "https://github.com/t-i-0414/react-reversi/issues",
    "email": "gutlo.ol0414@gmail.com"
  },
  "repository": "git@github.com:t-i-0414/react-reversi.git",
  "license": "MIT",
