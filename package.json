  "scripts": {
    "build": "NODE_OPTIONS='--openssl-legacy-provider' react-scripts build",
    "build-storybook": "NODE_OPTIONS='--openssl-legacy-provider' build-storybook -s public",
    "cypress:open": "NODE_OPTIONS='--openssl-legacy-provider' cypress open",
    "cypress:run": "NODE_OPTIONS='--openssl-legacy-provider' cypress run -s cypress/e2e/game.cy.ts cypress open",
    "eject": "react-scripts eject",
    "postinstall": "typesync",
    "lint": "yarn lint:es && yarn lint:style",
    "lint:conflict": "npx eslint --print-config .eslintrc.js | npx eslint-config-prettier-check",
    "lint:es": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:es:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "yarn lint:es:fix && yarn lint:style:fix",
    "lint:style": "stylelint 'src/**/*.{css,scss,jsx,ts,tsx}'",
    "lint:style:fix": "stylelint --fix 'src/**/*.{css,scss,jsx,ts,tsx}'",
    "prepare": "husky install",
    "start": "NODE_OPTIONS='--openssl-legacy-provider' react-scripts start",
    "storybook": "NODE_OPTIONS='--openssl-legacy-provider' start-storybook -p 6006 -s public",
    "test": "NODE_OPTIONS='--openssl-legacy-provider' react-scripts test",
    "type-check": "yarn tsc --noEmit --skipLibCheck"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "sort-package-json": "^2.4.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.1.17",
    "@storybook/addon-essentials": "^6.1.17",
    "@storybook/addon-links": "^6.1.17",
    "@storybook/node-logger": "^6.1.17",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.17",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^12.1.7",
    "@types/eslint": "7.2.4",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.14.31",
    "@types/prettier": "^2.1.2",
    "@types/react": "^16.9.52",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.11",
    "@types/react-test-renderer": "^17.0.0",
    "@types/redux-logger": "^3.0.8",
    "@types/styled-components": "^5.1.4",
    "@types/stylelint": "^9.10.1",
    "@types/testing-library__jest-dom": "^5.9.4",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "cypress": "^12.6.0",
    "eslint": "8.34.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.2",
    "prettier": "^2.1.2",
    "prettier-stylelint": "^0.4.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^7.4.2",
    "react-redux": "^7.2.2",
    "react-scripts": "5.0.1",
    "react-test-renderer": "^17.0.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.0",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.1.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "4.2.4",
    "typesync": "^0.7.0"
  }
}
